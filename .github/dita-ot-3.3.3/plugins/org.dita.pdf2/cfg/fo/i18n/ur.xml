<configuration>
  <alphabet char-set="Urdu">
    <character-set>
      <!-- Overlap with Arabic block -->
      <character-range>
        <start include="yes">&#x0600;</start>
        <end include="yes">&#x07FF;</end>
      </character-range>
      <!-- Alphabetic Presentation Forms block -->
      <character-range>
        <start include="yes">&#xFB50;</start>
        <end include="yes">&#xFEFF;</end>
      </character-range>
    </character-set>
  </alphabet>
  <alphabet char-set="SymbolsSuperscript">
    <character-set>
      <!-- Copyright -->
      <character>&#169;</character>
      <!-- Registered Trademark -->
      <character>&#174;</character>
      <!-- Trademark -->
      <character>&#8482;</character>
      <!-- Service mark -->
      <character>&#2120;</character>
    </character-set>
  </alphabet>
  <alphabet char-set="SubmenuSymbol">
    <character-set>
      <character>&#x27A4;</character>
    </character-set>
  </alphabet>
</configuration>
