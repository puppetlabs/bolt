      </xs:sequence>
      <xs:attributeGroup ref="namedetails.attributes"/>
   </xs:complexType>
   <xs:group name="namedetails.content">
      <xs:sequence>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="organizationnamedetails"/>
