      <define name="schemeref.content">
        <optional>
          <ref name="topicmeta"/>
        </optional>
        <zeroOrMore>
          <ref name="data.elements.incl"/>
        </zeroOrMore>
      </define>
      <define name="schemeref.attributes">
        <optional>
          <attribute name="navtitle"/>
        </optional>
