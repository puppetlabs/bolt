      <ref name="mi"/>
      <ref name="mn"/>
      <ref name="mo"/>
      <ref name="mtext"/>
      <ref name="mspace"/>
      <ref name="ms"/>
    </choice>
  </define>
  <define name="token.content">
    <choice>
      <ref name="mglyph"/>
      <ref name="malignmark"/>
